.banner {
  margin-top: 30rpx;
}

swiper {
  height: 470rpx;
}

swiper-item image {
  width: 690rpx;
  height: 430rpx;
  border-radius: 20rpx;
}

.out {
  margin-top: 10rpx;
}

.out .row {
  border: 2px solid #5aa85e;
  border-radius: 20rpx;
  height: 140rpx;
  display: flex;
  margin-bottom: 15rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
  padding-left: 20rpx;
  padding-top: 15rpx;
}

.out .row .pic {
  width: 100rpx;
  height: 130rpx;
}

.out .row .pic .img {
  width: 100%;
  height: 100%;
}

.out .row .text {
  width: 450rpx;
  height: 100rpx;
  margin-left: 20rpx;
  padding: 10rpx;
}

.out .row .text .title {
  color: #0c7439;
}

.out .row .text .info {
  margin-top: 10rpx;
  font-size: 28rpx;
  color: rgba(138, 135, 135, 0.938);
}

